<!DOCTYPE html>
<html>
<head>
  <title>Cmsonrails</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container-fluid">
  <div class="col-md-12">
    <div class="col-md-2">
      <ul>
        <li><%= link_to 'Home', root_path %></li>
        <li><%= link_to 'Banner', banners_path %></li>
        <li><%= link_to 'Conteudo', conteudos_path %></li>
        <li>Categoria</li>
        <li><%= link_to 'Contato', contatos_path %></li>
        <li>Fale Conosco</li>
      </ul>
    </div>
    <div class="col-md-8">
      <%= yield %>
    </div>
  </div>
</div>
</body>
</html>
