   <p id="notice"><%= notice %></p>

    <h4>Listing Banners</h4>

    <table class="mdl-data-table mdl-js-data-table mdl-data-table--selectable mdl-shadow--2dp">
      <thead>
        <tr>
          <th class="mdl-data-table__cell--non-numeric">Imagem</th>
          <th class="mdl-data-table__cell--non-numeric">Url</th>
          <th class="mdl-data-table__cell--non-numeric">Ações</th>
        </tr>
      </thead>

      <tbody>
        <% @banners.each do |banner| %>
          <tr>
            <td class="mdl-data-table__cell--non-numeric"><%= image_tag banner.image.url(:thumb) %></td>
            <td class="mdl-data-table__cell--non-numeric"><%= banner.url %></td>
            <td class="mdl-data-table__cell--non-numeric"><%= link_to 'Edit', edit_banner_path(banner), class: 'mdl-button mdl-js-button mdl-button--primary' %></td>
            <td class="mdl-data-table__cell--non-numeric"><%= link_to 'Destroy', banner, class: 'mdl-button mdl-js-button mdl-button--accent', method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <%= link_to 'New Banner', new_banner_path, class: 'mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect dist-bt' %>
