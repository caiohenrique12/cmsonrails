  <div class="panel panel-default">
    <div class="panel-heading">Listing Contatos</div>
    <div class="panel-body">
      <% @contatos.each do |contato| %>
        <div class="form-group">
          <h3><%= contato.endereco %></h3>
          <hr>
        </div>
        <div class="form-group">
          <h4><%= contato.telefone %></h4>
          <hr>
        </div>
        <div class="form-group">
          <h4><%= contato.celular %></h4>
          <hr>
        </div>
        <div class="form-group">
          <h4><%= contato.email %></h4>
          <hr>
        </div>
        <div class="form-group">
          <h5><%= contato.informacao %></h5>
          <hr>
        </div>

        <%= link_to 'Edit', edit_contato_path(contato), class: 'btn btn-success' %>
        <!--<%= link_to 'Destroy', contato, class: 'btn btn-danger', method: :delete, data: { confirm: 'Are you sure?' } %>-->
      <% end %>
    </div>
  </div>

  <%= link_to 'New Contato', new_contato_path, class: 'btn btn-info' %>
