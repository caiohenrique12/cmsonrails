  <% if @contato.errors.any? %>
    <div id="error_explanation">
      Há erros no formulário, por favor verifique.
    </div>
  <% end %>

  <%= form_for(@contato) do |f| %>
    <div class="field">
      <%= f.label :endereco %><br>
      <%= f.text_field :endereco %>
      <%= error_tag @contato, :endereco %>
    </div>
    <div class="field">
      <%= f.label :telefone %><br>
      <%= f.text_field :telefone %>
      <%= error_tag @contato, :telefone %>
    </div>
    <div class="field">
      <%= f.label :celular %><br>
      <%= f.text_field :celular %>
      <%= error_tag @contato, :celular %>
    </div>
    <div class="field">
      <%= f.label :email %><br>
      <%= f.text_field :email %>
      <%= error_tag @contato, :email %>
    </div>
    <div class="field">
      <%= f.label :informacao %><br>
      <%= f.text_area :informacao %>
      <%= error_tag @contato, :informacao %>
    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
  <% end %>
